<% layout('layout/boilerplate') %>
    <h1>Book id: <%= book.id %>
    </h1>
    <h2>
        <%= book.title%> - <%= book.username%>
    </h2>
    <form method="POST" action='/books/<%= book.id %>?_method=DELETE'>
        <a href="/books" class="btn btn-secondary">All Books</a>

        <button class="btn btn-danger">Delete</button>
    </form>